{
  "name": "css-frameworks-ca",
  "version": "1.1.1",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-cli": "cypress run",
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier --write 'src/**/*.js'",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "lint": "eslint src/**/*.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.15.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-jest": "^28.8.3",
    "husky": "^9.1.6",
    "jest": "^29.2.0",
    "lint-staged": "^15.2.10",
    "live-server": "^1.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "anymatch": "^3.1.3",
    "bootstrap-dark-5": "^1.1.3",
    "braces": "^3.0.3",
    "chokidar": "^4.0.1",
    "connect": "^3.7.0",
    "eslint": "^8.57.1",
    "finalhandler": "^1.3.1",
    "micromatch": "^4.0.8"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
